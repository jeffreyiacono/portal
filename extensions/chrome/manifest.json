{
  // Required
  "name"             : "Portal",
  "version"          : "0.0.1",
  "manifest_version" : 2,

  // Recommended
  "description"    : "making it simple to share browser tabs between friends so there is more time for cake. you monster.",
  "icons"          : {
    "32" : "images/icon32.png",
    "48" : "images/icon48.png"
  },

  "browser_action": {
    "default_icon"  : "images/icon19.png", // optional
    "default_title" : "Portal",            // optional; shown in tooltip
    "default_popup" : "popup.html"         // optional
  },

  // Add any of these that you need
  "background": {},
  "homepage_url"           : "https://github.com/jeffreyiacono/portal",
  "minimum_chrome_version" : "18.0",
  "permissions"            : [],
  "requirements"           : {},
  "update_url"             : "http://nonexistant-page.html"
}
